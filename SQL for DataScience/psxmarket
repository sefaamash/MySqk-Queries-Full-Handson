                        c.execute('''INSERT INTO CableElecGoods VALUES(?,?,?,?,?,?,?,?)''',(scripb,ldcb,openb,highb,lowb,currb,chnb,volb))
                        conn.commit()
                        print("Completed")
                    
        except  Exception as e:
                    scripg=None
                    ldcg=None
                    openg=None
                    highg=None
                    lowg=None
                    currg=None
                    chng=None
                    volg=None
                    scripr=None
                    ldcr=None
                    openr=None
                    highr=None
                    lowr=None
                    currr=None
                    chnr=None
                    volr=None
                    scripb=None
