                    conn.commit()
                    #.......................BLUE..........................
                print('**************************BLUE***************************') 
                for det in Spansoup2.find_all('tr',{'class':'blue-text-td'}):
                    scripb=det.find_all('td')[0].text
                    print(scripb)
                    ldcb=det.find_all('td')[1].text
                    print(ldcb)
                    openb=det.find_all('td')[2].text
