                print('**************************GREEN***************************')
                for det in Spansoup2.find_all('tr',{'class':'green-text-td'}):
                    scripg=det.find_all('td')[0].text
                    print(scripg)
                    ldcg=det.find_all('td')[1].text
                    print(ldcg)
