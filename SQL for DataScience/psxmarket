                    print(currr)
                    chnr=det.find_all('td')[6].text
                    print(chnr)
                    volr=det.find_all('td')[7].text
                    print(volr)
                    c.execute('''INSERT INTO  MutualFunds VALUES(?,?,?,?,?,?,?,?)''',(scripr,ldcr,openr,highr,lowr,currr,chnr,volr))
                    conn.commit()
                    #.......................BLUE..........................
