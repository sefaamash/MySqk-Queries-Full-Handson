                    chng=det.find_all('td')[6].text
                    print(chng)
                    volg=det.find_all('td')[7].text
                    print(volg)
                    c.execute('''INSERT INTO MutualFunds VALUES(?,?,?,?,?,?,?,?)''',(scripg,ldcg,openg,highg,lowg,currg,chng,volg))
                    conn.commit()

                    #.......................RED..........................
